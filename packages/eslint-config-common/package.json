{
  "name": "eslint-config-common",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "js:check": "eslint --ignore-path .eslintignore '../../**/*.{js,jsx,ts,tsx}'",
    "js:fix": "eslint --fix '../../**/*.{js,jsx,ts,tsx}'",
    "sol:check": "solhint --config ./.solhint.json '../../**/*.sol'",
    "sol:fix": "solhint --config ./.solhint.json '../../**/*..sol' --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-turbo": "^1.10.6",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "0.6.12",
    "solhint": "^4.1.1"
  },
  "exports": {
    ".": {
      "require": "./index.cjs"
    }
  }
}
